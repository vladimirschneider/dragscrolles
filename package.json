{
  "author": "Vladimir Schneider <vladimir.tbb@gmail.com>",
  "name": "dragscrolles",
  "description": "JavaScript Libary for scrolling via holding mouse button (\"drag and drop\" or \"click and hold\"). It has no dependencies and is written in vanilla JavaScript.",
  "version": "1.0.3",
  "keywords": [
    "dragscroll",
    "scroll",
    "scrolling",
    "mouse",
    "hold",
    "drag-n-drop",
    "drag-and-drop",
    "click-n-hold",
    "click-n-scroll",
    "click-and-hold",
    "click-and-scroll",
    "drag",
    "viewport"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/vladimirschneider/dragscrolles"
  },
  "scripts": {
    "start:prod": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.prod.babel.js",
    "start:example": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.example.babel.js"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2"
  }
}
